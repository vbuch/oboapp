{
  "name": "oboapp-ingest",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "crawl": "tsx crawl",
    "ingest": "tsx ingest",
    "notify": "tsx notify",
    "categorize": "tsx categorize",
    "gtfs-stops": "tsx ingest gtfs-stops",
    "pipeline:emergent": "tsx pipeline --emergent",
    "pipeline:all": "tsx pipeline --all",
    "emulators": "firebase emulators:start --import=./emulator-data --export-on-exit",
    "emulators:fresh": "firebase emulators:start",
    "emulator:seed": "tsx scripts/seed-emulator.ts",
    "emulator:check": "tsx scripts/check-emulator-data.ts",
    "emulator:clear": "tsx scripts/clear-emulator-data.ts",
    "capture-fixtures": "tsx scripts/capture-api-responses.ts",
    "capture:firestore": "tsx scripts/capture-from-firestore.ts",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build:docker": "tsc -p tsconfig.docker.json && tsc-alias -p tsconfig.docker.json",
    "prebuilt:crawl": "node dist/crawl.js",
    "prebuilt:ingest": "node dist/ingest.js",
    "prebuilt:notify": "node dist/notify.js",
    "prebuilt:gtfs-stops": "node dist/ingest.js gtfs-stops",
    "prebuilt:pipeline:emergent": "node dist/pipeline.js --emergent",
    "prebuilt:pipeline:all": "node dist/pipeline.js --all"
  },
  "dependencies": {
    "@google/genai": "^1.33.0",
    "@react-google-maps/api": "^2.20.7",
    "@turf/turf": "^7.3.1",
    "adm-zip": "^0.5.16",
    "commander": "^14.0.2",
    "csv-parse": "^5.6.0",
    "dotenv": "^17.2.3",
    "firebase": "^12.6.0",
    "firebase-admin": "^13.6.0",
    "playwright": "^1.57.0",
    "proj4": "^2.20.2",
    "turndown": "^7.2.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^20",
    "@types/turndown": "^5.0.6",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "*",
    "eslint-config-next": "^16.0.10",
    "firebase-tools": "^15.5.1",
    "happy-dom": "^20.0.11",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.15"
  }
}
